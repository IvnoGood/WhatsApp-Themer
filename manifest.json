{
  "manifest_version": 3,
  "name": "Whatsapp themer",
  "description": "An extension that permitrs the user to customize WhatsappWeb how he wants",
  "version": "1.0",
  "permissions": ["scripting", "activeTab"],
  "icons": {
    "32": "./icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["service-worker.js"],
      "css": ["stylesheet.css"]
    }
  ]
}
